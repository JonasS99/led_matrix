void MainFsm_Init(void)
{


}

void MainFsm_StateMachine(void)
{
	static fsm_t state = FSM_HOME;

	switch(state)
	{
		case FSM_HOME:
		{
			break;
		}

		case FSM_STATIC_LED:
		{
			break;
		}

		case FSM_DYNMAMIC_LED:
		{
			break;
		}

		case FSM_REGULATOR:
		{
			break;
		}

		case FSM_FORMS:
		{
			break;
		}

		case FSM_FPGA:
		{
			break;
		}

		case FSM_TETRIS:
		{
			break;
		}
	}

}




